#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/lighthouse@11.4.0/node_modules/lighthouse/core/scripts/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/lighthouse@11.4.0/node_modules/lighthouse/core/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/lighthouse@11.4.0/node_modules/lighthouse/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/lighthouse@11.4.0/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/lighthouse@11.4.0/node_modules/lighthouse/core/scripts/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/lighthouse@11.4.0/node_modules/lighthouse/core/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/lighthouse@11.4.0/node_modules/lighthouse/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/lighthouse@11.4.0/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../core/scripts/manual-chrome-launcher.js" "$@"
else
  exec node  "$basedir/../../core/scripts/manual-chrome-launcher.js" "$@"
fi
