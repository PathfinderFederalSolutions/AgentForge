#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/@lhci+cli@0.13.0/node_modules/@lhci/cli/src/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/@lhci+cli@0.13.0/node_modules/@lhci/cli/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/@lhci+cli@0.13.0/node_modules/@lhci/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/@lhci+cli@0.13.0/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/@lhci+cli@0.13.0/node_modules/@lhci/cli/src/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/@lhci+cli@0.13.0/node_modules/@lhci/cli/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/@lhci+cli@0.13.0/node_modules/@lhci/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/@lhci+cli@0.13.0/node_modules:/Users/baileymahoney/AgentForge/ui/tactical-dashboard/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@lhci/cli/src/cli.js" "$@"
else
  exec node  "$basedir/../@lhci/cli/src/cli.js" "$@"
fi
